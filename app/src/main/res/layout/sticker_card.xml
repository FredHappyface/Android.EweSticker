<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="@dimen/sticker_padding"
	app:cardCornerRadius="@dimen/zero">

	<ImageButton
		android:id="@+id/stickerButton"
		android:layout_width="@dimen/pack_dimens"
		android:layout_height="@dimen/pack_dimens"
		android:background="@color/bg"
		android:contentDescription="@string/pack_icon"
		android:cropToPadding="true"
		android:scaleType="fitCenter" />
</FrameLayout>
